 useEffect, useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { BsSearch } from \"react-icons/bs\";\nimport { BsPerson } from \"react-icons/bs\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { signout } from '../actions/userActions';\n\nexport default function Navbar(props) {\n  const userSignin = useSelector((state) => state.userSignin);\n  const { userInfo } = userSignin;\n  const [currentTime,setCurrentTime] = useState(new Date())\n  const [status,setStatus] = useState('Chao')\n  \n  useEffect(() =>{\n    if(currentTime.getHours() >= 0 && currentTime.getHours() <= 12) {\n      setStatus('Chào buổi sáng')\n    } else if (currentTime.getHours() >= 13 && currentTime.getHours() <= 18) {\n      setStatus('Chào buồi chiều')\n    } else {\n      setStatus('Chào buồi tối')\n    }\n    var timerID = setInterval( () => tick(), 1000 );\n    // return function cleanup() {\n    //   clearInterval(timerID);\n    // }; \n   },[currentTime.getHours()])\n    function tick() {\n      setCurrentTime(new Date());\n     }\n  const dispatch = useDispatch();\n  const signoutHandler = () => {\n    dispatch(signout());\n  };\n  return (\n    <div>\n    <nav class=\"navbar navbar-light bg-light \">\n        <Link to=\"/\" class=\"navbar-brand\">RecommendationNews</Link>\n        <ul class=\"nav navbar-nav navbar-right flex-row\">\n        { userInfo ? (\n          <div className = \"login pl-2 d-flex flex-column\">\n          <Link to=\"/profile\" className='font-weight-bold text-decoration-none'><small> {status } {userInfo.name} </small></Link>\n          <Link to=\"#signout\" onClick={signoutHandler} style = {{color : \"red\"}} className='text-decoration-none'>\n            <small>Thoát</small>  \n          </Link>\n          </div> \n        ) : (\n          <li className='pl-2 py-2 my-2'>\n          <Link to=\"/signin\">\n            <BsPerson size={24}/>\n          </Link>\n          </li>\n        )\n        }\n        </ul>\n    </nav>\n    </div>\n  );\n}","/Users/user/Desktop/Recommend/recommendation/src/screens/SigninScreen.js",["108","109","110"],"import React, { useEffect, useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport { signin } from '../actions/userActions';\n// import MessageBox from '../components/MessageBox';\nimport './Signin.css'\nimport DjangoCSRFToken from 'django-react-csrftoken'\n\nexport default function SigninScreen(props) {\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n\n  const redirect = props.location.search\n    ? props.location.search.split('=')[1]\n    : '/';\n\n  const userSignin = useSelector((state) => state.userSignin);\n  const { userInfo, error } = userSignin;\n\n  const dispatch = useDispatch();\n  const submitHandler = (e) => {\n    e.preventDefault();\n    dispatch(signin(email, password));\n  };\n  useEffect(() => {\n    if (userInfo) {\n      props.history.push(redirect);\n    }\n  }, [props.history, redirect, userInfo]);\n  return (\n    <div className=\"login-form\">    \n    <form className='form' onSubmit={submitHandler}>\n      <DjangoCSRFToken/>\n    \t<h4 className=\"modal-title\">ĐĂNG NHẬP</h4>\n        <div className=\"form-group\">\n            <input type=\"text\" className=\"form-control\" placeholder=\"Username\" required=\"required\" onChange={(e) => setEmail(e.target.value)}/>\n        </div>\n        <div className=\"form-group\">\n            <input type=\"password\" className=\"form-control\" placeholder=\"Password\" required=\"required\" onChange={(e) => setPassword(e.target.value)}/>\n        </div>\n        <div className=\"form-group small clearfix\">\n            <label className=\"checkbox-inline\"><input type=\"checkbox\" /> Remember me </label>\n            <a href=\"#\" className=\"forgot-link\">Quên mật khẩu?</a>\n        </div> \n        <input type=\"submit\" className=\"button btn-primary btn-block btn-lg \" value=\"Đăng nhập\" />              \n    </form>\t\t\t\n    <div className=\"text-center small\">Chưa có tài khoản? <Link to='signup'>Đăng kí</Link></div>\n</div>\n  );\n}\n","/Users/user/Desktop/Recommend/recommendation/src/component/BottomNav.js",["111","112","113"],"import React, { useEffect, useState } from \"react\";\nimport { BsSearch } from \"react-icons/bs\";\nimport { NavLink } from \"react-router-dom\";\nimport \"./BottomNav.css\";\nimport Axios from 'axios'\nexport default function BottomNav() {\n  const [isopen, setopenSearch] = useState(false);\n  const [idlist,setIdlist] = useState([]);\n  const openSearch = (e) => {\n    e.preventDefault();\n    if (isopen) {\n      setopenSearch(false);\n    } else {\n      setopenSearch(true);\n    }\n  };\n  useEffect( async () => {\n    const { data }  = await Axios.get('https://recommendationnews1.herokuapp.com/api/category');\n    // console.log(data)\n    setIdlist([...data])\n  },[])\n  return (\n    <div>\n      <nav className=\"navbar py-1 px-1 mb-3 bottom-header bg-info list-unstyled \">\n        <ul className='navbar-nav d-flex flex-row '>\n        <NavLink exact to=\"/\" className=\"Nav float-left text-decoration-none\" activeClassName=\"nav-active\">\n          <li className=\"px-2 py-2 border-white \">TIN CỦA BẠN</li>\n        </NavLink>\n        <NavLink className=\"text-decoration-none\"\n          exact\n          to=\"/hot_article\"\n          className=\"Nav float-left\"\n          activeClassName=\"nav-active\"\n        >\n          <li className=\"px-2 py-2 border-white \">NÓNG</li>\n        </NavLink>\n        <NavLink className=\"text-decoration-none\"\n          exact\n          to=\"/new_article\"\n          className=\"Nav float-left\"\n          activeClassName=\"nav-active\"\n        >\n          <li className=\"px-2 py-2 border-white \">MỚI</li>\n        </NavLink>\n        {\n          idlist.map((category => \n          <NavLink exact to = {`/${category.name}`} className=\"